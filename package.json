{
  "name": "trauma-in-a-can",
  "version": "1.0.0",
  "description": "Trauma In A Canâ„¢ - Full-stack e-shop for bottled traumatic experiences with Redis cache",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "dev": "concurrently \"pnpm dev:backend\" \"pnpm dev:frontend\"",
    "dev:backend": "pnpm --filter backend run start:dev",
    "dev:frontend": "pnpm --filter frontend run dev",
    "dev:infra": "docker-compose -f docker-compose.dev.yml up -d",
    "build": "pnpm run build:backend && pnpm run build:frontend",
    "build:backend": "pnpm --filter backend run build",
    "build:frontend": "pnpm --filter frontend run build",
    "docker:up": "docker-compose up --build -d",
    "docker:up:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker-compose down",
    "docker:down:volumes": "docker-compose down -v",
    "docker:logs": "docker-compose logs -f",
    "docker:logs:backend": "docker-compose logs -f backend",
    "docker:logs:frontend": "docker-compose logs -f frontend",
    "docker:restart": "docker-compose restart",
    "db:seed": "docker exec -i product-catalog-postgres psql -U postgres -d product_catalog -f /docker-entrypoint-initdb.d/../seed.sql"
  },
  "keywords": [
    "trauma-in-a-can",
    "e-commerce",
    "redis",
    "cache",
    "nestjs",
    "nextjs",
    "postgresql",
    "docker"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vochechule/Trauma-In-A-Can.git"
  },
  "author": "vochechule",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
